<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Version>2.4.0</Version>
    <Company>Neptuo</Company>
    <Product>FileUpload</Product>
    <RepositoryUrl>https://github.com/maraf/FileUpload</RepositoryUrl>
    <PackageProjectUrl>https://github.com/maraf/FileUpload</PackageProjectUrl>
    <RootNamespace>FileUpload</RootNamespace>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
    <DockerfileContext>..\..</DockerfileContext>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="BuildBundlerMinifier" Version="3.2.449" />
    <PackageReference Include="Microsoft.VisualStudio.Azure.Containers.Tools.Targets" Version="1.10.9" />
    <PackageReference Include="Neptuo" Version="6.0.1" />
  </ItemGroup>

</Project>
